{
  "name": "percy-seleniumjs",
  "version": "0.1.0-alpha.1",
  "description": "Unofficial SeleniumJS SDK for Percy. Works with Protractor too.",
  "main": "lib/index.js",
  "scripts": {
    "build": "rimraf lib/* && babel src --out-dir lib --delete-dir-on-start",
    "lint": "eslint ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 79,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/4Catalyzer/percy-seleniumjs.git"
  },
  "files": [
    "lib"
  ],
  "author": "4Catalyzer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/4Catalyzer/percy-seleniumjs/issues"
  },
  "homepage": "https://github.com/4Catalyzer/percy-seleniumjs#readme",
  "peerDependencies": {
    "selenium-webdriver": "^3.0.0 || ^4.0.0-alpha"
  },
  "devDependencies": {
    "@4c/babel-preset-4catalyzer": "^3.1.1",
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "eslint": "^5.6.1",
    "eslint-config-4catalyzer": "^0.4.4",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.0",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "mime-types": "^2.1.21",
    "percy-client": "^3.0.1",
    "walk": "^2.3.14"
  },
  "release": {
    "publishDir": "lib"
  }
}
